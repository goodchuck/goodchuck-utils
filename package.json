    "dayjs": "^1.11.19"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "typescript": "^5.9.3",
    "@types/react": "^19.2.8",
    "@vitest/ui": "^4.0.16",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
